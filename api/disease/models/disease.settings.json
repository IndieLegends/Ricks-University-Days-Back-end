{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "diseases",
  "info": {
    "name": "disease"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "unique": true,
      "model": "disease-name",
      "via": "disease"
    },
    "transmissions": {
      "collection": "disease-transmission",
      "via": "disease"
    },
    "symptoms": {
      "collection": "disease-symptom",
      "via": "disease"
    },
    "treatments": {
      "collection": "disease-treatment",
      "via": "disease"
    },
    "causes": {
      "collection": "disease-cause",
      "via": "disease"
    }
  }
}
