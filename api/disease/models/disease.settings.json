{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "diseases",
  "info": {
    "name": "disease"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "model": "disease-name"
    },
    "disease_causes": {
      "collection": "disease-cause",
      "via": "diseases",
      "dominant": true
    },
    "disease_symptoms": {
      "collection": "disease-symptom",
      "via": "diseases",
      "dominant": true
    },
    "disease_transmissions": {
      "collection": "disease-transmission",
      "via": "diseases",
      "dominant": true
    },
    "disease_treatments": {
      "collection": "disease-treatment",
      "via": "diseases",
      "dominant": true
    },
    "cards": {
      "via": "disease",
      "collection": "card"
    }
  }
}
