{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "model": "disease-name"
    },
    "type": {
      "model": "project-type"
    },
    "gold": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "honor": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "passion": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "visible": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
