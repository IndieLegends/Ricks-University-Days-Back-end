{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "user_infos",
  "info": {
    "name": "user_info"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "nickname": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "cards": {
      "collection": "card",
      "via": "user_infos",
      "dominant": true
    },
    "projects": {
      "collection": "project",
      "via": "user_infos",
      "dominant": true
    },
    "money": {
      "type": "integer",
      "required": true
    },
    "honor": {
      "type": "integer",
      "required": true
    },
    "passion": {
      "type": "integer",
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
