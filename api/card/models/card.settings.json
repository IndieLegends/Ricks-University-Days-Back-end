{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "cards",
  "info": {
    "name": "card"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "cause": {
      "model": "disease-cause"
    },
    "transmission": {
      "model": "disease-transmission"
    },
    "symptom": {
      "model": "disease-symptom"
    },
    "treatment": {
      "model": "disease-treatment"
    },
    "gold": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "honor": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "passion": {
      "required": true,
      "type": "integer",
      "default": 0
    },
    "isdeleted": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "user_infos": {
      "via": "cards",
      "collection": "user-info"
    }
  }
}
